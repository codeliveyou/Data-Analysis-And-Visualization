= open(\n\u001b[0m\u001b[1;32m    857\u001b[0m                 \u001b[0mhandle\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m    858\u001b[0m                 \u001b[0mioargs\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0mmode\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n","\u001b[0;31mFileNotFoundError\u001b[0m: [Errno 2] No such file or directory: '/content/gdrive/MyDrive/ICE-V2-Dataset/Datasets/data_7car_data.csv'"]}],"source":["# Import necessary libraries.\n","import pandas as pd\n","\n","# path to data.\n","path = '/content/gdrive/MyDrive/ICE-V2-Dataset/Datasets/data_7car_data.csv'\n","\n","# Load data.\n","df = pd.read_csv(path)\n","\n","# Delete year.\n","df = df.drop(columns=['year'])\n","\n","# Numerical columns to normalize\n","col_to_norm = df.select_dtypes(include=['float','int'])\n","\n","# Normalize the columns\n","for col in col_to_norm:\n","    min_val = df[col].min()\n","    max_val = df[col].max()\n","    df[col] = (df[col] - min_val) / (max_val - min_val)\n","\n","# Filter the dataframe\n","df = df[(df['engine_size'] / df['fuel_efficiency'] < 1)].copy()\n","\n","# Calculate the efficiency ratio\n","df['efficiency_ratio'] = df['engine_size'] / df['fuel_efficiency'].astype(float)\n","\n","# Calculate score based the given formula.\n","df['score'] = (0.2*df['fuel_efficiency']+0.2*df['engine_size']+0.1*df['horsepower']+0.1*df['weight']+0.1*df['acceleration']-0.2*df['efficiency_ratio'])\n","\n","# Round with two decimals.\n","df['score'] = df['score'].round(2)\n","\n","# Sort by score\n","df_sorted = df.sort_values(by='score', ascending = False)\n","\n","# Tops car acording to formula.\n","df.head(3)"]}]}